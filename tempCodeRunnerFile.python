# ================================ 
# 1. Line Plot using Matplotlib 
# ================================
import matplotlib.pyplot as plt 
# Line Plot 
x = [1, 2, 3, 4, 5] 
y = [2, 3, 5, 7, 11] 
plt.figure() 
plt.plot(x, y) 
plt.title("Line Plot Example") 
plt.xlabel("X-axis") 
plt.ylabel("Y-axis") 
plt.show()